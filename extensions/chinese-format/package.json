{
  "name": "mini-chinese-format",
  "displayName": "Mini Chinese Format",
  "version": "0.0.1",
  "description": "",
  "main": "./dist/extension.js",
  "icon": "assets/logo.png",
  "scripts": {
    "compile": "pnpm run check-types && node esbuild.js",
    "watch:esbuild": "node esbuild.js --watch",
    "package": "pnpm run check-types && node esbuild.js --production",
    "check-types": "tsc --noEmit"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "mini-chinese-format.chineseFormat",
        "title": "%mini-chinese-format.title%"
      }
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "pangu": "^4.0.7"
  },
  "devDependencies": {
    "@types/pangu": "^4.0.2",
    "@types/vscode": "1.93.0",
    "esbuild": "0.24.0"
  }
}

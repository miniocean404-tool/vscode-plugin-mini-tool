{
  "name": "mini-code-snap",
  "displayName": "Mini Code Snap",
  "version": "0.0.1",
  "description": "",
  "main": "./dist/extension.js",
  "icon": "./assets/logo.png",
  "scripts": {
    "compile": "pnpm run check-types && node esbuild.js",
    "watch:esbuild": "node esbuild.js --watch",
    "package": "pnpm run check-types && node esbuild.js --production",
    "check-types": "tsc --noEmit"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "mini-code-snap.codeSnap",
        "title": "%mini-code-snap.title%"
      }
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dom-to-image-even-more": "^1.0.4"
  },
  "devDependencies": {}
}

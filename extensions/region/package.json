{
  "name": "mini-region",
  "displayName": "Mini Region",
  "version": "0.0.1",
  "description": "",
  "main": "./dist/extension.js",
  "icon": "./assets/logo.png",
  "scripts": {
    "compile": "pnpm run check-types && node esbuild.js",
    "watch:esbuild": "node esbuild.js --watch",
    "package": "pnpm run check-types && node esbuild.js --production",
    "check-types": "tsc --noEmit"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "mini-region.addRegion",
        "title": "%mini-region.title%"
      }
    ],
    "keybindings": [
      {
        "command": "mini-region.addRegion",
        "key": "ctrl+alt m",
        "mac": "cmd+alt m",
        "when": "editorTextFocus"
      }
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {}
}
